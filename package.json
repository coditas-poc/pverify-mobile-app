{
  "name": "PVerify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "postinstall": "npx jetify",
    "lint": "tslint --fix src/**/*.{ts,tsx}",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "ios:debug": "cp src/App/Config/index.dev.ts src/App/Config/index.ts && react-native run-ios",
    "android:dedug": "cp src/App/Config/index.dev.ts src/App/Config/index.ts && react-native run-android",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:bundleRelease": "cd android && ./gradlew bundleRelease",
    "android:clean": "cd android && ./gradlew clean",
    "ios:devicelist": "xcrun simctl list devices"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/datetimepicker": "^2.1.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "axios": "^0.19.0",
    "formik": "^2.1.2",
    "ramda": "^0.26.1",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-camera": "^3.15.1",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-reanimated": "^1.4.0",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3",
    "react-redux": "^7.1.3",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "reduxsauce": "^1.1.1",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/runtime": "^7.5.0",
    "@react-native-community/eslint-config": "^0.0.3",
    "@types/jest": "^24.0.18",
    "@types/ramda": "^0.26.39",
    "@types/react": "^16.9.2",
    "@types/react-native": "^0.60.14",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.9.0",
    "@types/yup": "^0.26.27",
    "babel-jest": "^24.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "husky": "^3.1.0",
    "jest": "^24.1.0",
    "metro-react-native-babel-preset": "0.54.1",
    "react-test-renderer": "16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^3.6.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
